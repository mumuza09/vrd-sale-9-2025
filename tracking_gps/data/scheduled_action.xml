<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="ir_cron_scheduled_line" model="ir.cron">
		<field name="name">Noti lineOA at 09:15 (Excluding Sun)</field>
		<field name="model_id" ref="model_schedule_line"/>
		<field name="state">code</field>
